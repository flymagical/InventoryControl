@model List<InventoryControl.Data.Views.vw_PredictedItem>

<table>
    <thead>
        <tr>
            <th>Bulan</th>
            <th>Tahun</th>
            <th>Bidang</th>
            <th>Nama Barang</th>
            <th>Satuan</th>
            <th>Jumlah</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() > 0)
        {
            @foreach (var iItem in Model)
            {
                <tr>
                    <td>@iItem.Bulan</td>
                    <td>@iItem.Tahun</td>
                    <td>@iItem.NamaBidang</td>
                    <td>@iItem.NamaItem</td>
                    <td>@iItem.NamaSatuan</td>
                    <td>@iItem.Jumlah</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td style="width:20%;height:50px" class="_center" colspan="6"><span class="text-danger">Tidak Ada Data</span> </td>

            </tr>
        }

    </tbody>

</table>